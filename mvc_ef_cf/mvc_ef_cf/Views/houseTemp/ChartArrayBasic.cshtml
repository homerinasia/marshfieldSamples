@model IEnumerable<mvc_ef_cf.Models.houseTemp>

@{

    var x = from a in Model
            select a.tds;
    var y = from a in Model
            select a.tempF;


    var myChart = new Chart(width: 900, height: 600)
       .AddTitle("House Temp")
       .AddSeries(chartType: "column",
          xValue: x.ToArray(),
          yValues: y.ToArray())
       .Write();

}